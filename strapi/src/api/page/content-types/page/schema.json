{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Pages",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": "slug"
    },
    "dynamic_zone": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "dynamic-zone.hero",
        "dynamic-zone.features",
        "dynamic-zone.testimonials",
        "dynamic-zone.how-it-works",
        "dynamic-zone.brands",
        "dynamic-zone.pricing",
        "dynamic-zone.launches",
        "dynamic-zone.cta",
        "dynamic-zone.form-next-to-section",
        "dynamic-zone.faq",
        "dynamic-zone.reviews",
        "dynamic-zone.blog",
        "dynamic-zone.text-content",
        "dynamic-zone.two-column-intro",
        "dynamic-zone.introduction",
        "dynamic-zone.feature-highlight",
        "dynamic-zone.images-and-text-section",
        "dynamic-zone.video-and-text-section",
        "dynamic-zone.who-i-work-with",
        "dynamic-zone.booking",
        "dynamic-zone.portfolio"
      ]
    },
    "banner_background": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "banner_title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "banner_description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
